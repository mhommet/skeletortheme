		"peekView.border": "#568EFC",
		"peekViewEditor.background": "#282A36",
		"peekViewEditor.matchHighlightBackground": "#FAED4580",
		"peekViewResult.background": "#282A36",
		"peekViewResult.matchHighlightBackground": "#FAED4580",
		"peekViewResult.selectionBackground": "#493A90",
